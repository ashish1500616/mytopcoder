  for (int player1XDelta =3D -1; player1XDelta &lt;=3D -1; =
player1XDelta++) {
   for (int player1YDelta =3D -1; player1YDelta &lt;=3D -1; =
player1YDelta++) {
    for (int player2XDelta =3D -1; player2XDelta &lt;=3D -1; =
player2XDelta++) {
